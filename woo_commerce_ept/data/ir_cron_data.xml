    <record id="ir_cron_parent_woo_product_process_child_cron"
            model="ir.cron">
        <field name="name">Woocommerce: Process Products Queue</field>
        <field name="model_id"
               ref="model_woo_product_data_queue_line_ept"/>
        <field name="state">code</field>
        <field name="code">model.woo_product_process_child_cron()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>

    <!-- Woo Order Process Cron -->
    <record id="process_woo_order_data_queue_parent_cron"
            model="ir.cron">
        <field name="name">WooCommerce:Process Orders Queue</field>
        <field name="active" eval="True"/>
        <field name="model_id"
               ref="model_woo_order_data_queue_line_ept"/>
        <field name="state">code</field>
        <field name="code">model.check_child_cron()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>

